{
  "name": "saga-transaction-manager",
  "version": "1.0.0",
  "description": "Transaction management in Saga model",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --verbose --no-cache --coverage --detectOpenHandles --config jest.config.json",
    "build": "tsc && tsc-alias",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "keywords": [
    "transaction",
    "manager"
  ],
  "author": "yuchun",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.4",
    "tsc-alias": "^1.8.10",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.4.5"
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md",
    "resource",
    "package.json"
  ],
  "dependencies": {
    "reflect-metadata": "^0.2.2"
  }
}